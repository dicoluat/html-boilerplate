<section class="section__network">
  <div class="container">
    <h2 class="heading2">{{ componentData.title ?? '...' }}</h2>
    <div class="card card__primary network__body">
      <div class="network__content">
        <div class="network__desc">{{ componentData.desc??'...' }}</div>
        <div class="network__link">
          <div class="buttons">
            <a
                href="{{ componentData.button.link ?? '#no-link' }}"
                class="button button__orange has-arrow">
              {{ componentData.button.title ?? '...' }}
            </a>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
